<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-cp .:../lib/* -Xms32m -Xmx384m -Dorg.apache.commons.logging.Log=org.openas2.logging.Log</exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs>../config/config.xml</exec.appArgs>
                <exec.mainClass>org.openas2.app.OpenAS2Server</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/home/javier/Documents/Projects/Greicodex/repos/OpenAs2App/Runtime/bin</exec.workingdir>
            </properties>
        </action>
